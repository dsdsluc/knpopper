div.tab-pane.fade(id="v-pills-ticket" role="tabpanel" aria-labelledby="v-pills-ticket-tab" tabindex="0")
    .support-ticket
        btn.shop-btn(onclick="modalAction('.ticket')") Add New Support

        // ticket-modal
        .modal-wrapper.ticket
            div.anywhere-away(onclick="modalAction('.ticket')")

            // change this
            .login-section.account-section.modal-main
                .review-form
                    .review-content
                        h5.comment-title Add New Ticket
                        .close-btn
                            img(src="clients/images/homepage-one/close-btn.png" onclick="modalAction('.ticket')" alt="close-btn")
                    form(action="/user/support-tickets" method="POST")
                        .review-form-name.address-form
                            label(for="ticket" class="form-label") First Name*
                            input#ticket.form-control(type="text" placeholder="Name" value=user.fullName)
                        .account-inner-form
                            .review-form-name
                                label(for="ticketaddress" class="form-label") Email Address*
                                input#ticketaddress.form-control(type="email" placeholder="email@gmail.com" value=user.email)
                            .review-form-name
                                label(for="ticketphone" class="form-label") Phone Number*
                                input#ticketphone.form-control(type="tel" placeholder="******" value=user.phone)
                        .review-form-name.address-form
                            label(for="ticketmassage" class="form-label") Description*
                            textarea#ticketmassage.form-control(cols="10" rows="3" placeholder="Write Here your Description" name="description")
                        .login-btn.text-center
                            button.shop-btn(type="submit" onclick="modalAction('.ticket')") Add Ticket Now

        .ticket-section
            table
                tbody
                    tr.table-row.table-top-row
                        td.table-wrapper
                            h5.table-heading NO
                        td.table-wrapper
                            .table-wrapper-center
                                h5.table-heading TIME
                        td.table-wrapper
                            .table-wrapper-center
                                h5.table-heading REPORT
                        td.table-wrapper
                            .table-wrapper-center
                                h5.table-heading ACTION
                    each item in supportTickets
                        tr.table-row.ticket-row
                            td.table-wrapper
                                p.ticker-number # #{item._id.toString().slice(-3)}
                            td.table-wrapper
                                .table-wrapper-center
                                    p.ticket-date  #{new Date(item.time).toLocaleDateString()}
                            td.table-wrapper
                                .table-wrapper-center
                                    p.ticket-info #{item.description}
                                        
                            td.table-wrapper
                                .table-wrapper-center.table-wrapper-img
                                    .comment-img
                                        span
                                            div.comment-img
